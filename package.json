{
  "name": "opensips-js",
  "version": "1.0.8",
  "description": "The JS package for opensips",
  "default": "src/index.ts",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "parcel demo/index.html --https",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts --fix --ignore-path .gitignore"
  },
  "engines": {
    "node": ">= 16",
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.4"
  },
  "precommit": "ts-check, lint",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/VoicenterTeam/opensips-js.git"
  },
  "author": "Bohdan Konechnyi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VoicenterTeam/opensips-js/issues"
  },
  "homepage": "https://github.com/VoicenterTeam/opensips-js#readme",
  "devDependencies": {
    "@parcel/config-default": "^2.8.3",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "parcel": "^2.8.3",
    "pre-commit": "^1.2.2",
    "process": "^0.11.10",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "generate-unique-id": "^2.0.1",
    "jssip": "3.4.0",
    "loglevel": "^1.8.1",
    "p-iteration": "^1.1.8"
  }
}
